<div class="row">
  <div class="col s12 m12">
    <div class="card-panel">
      <div class="row header center">
      <h6><b>TESTCASE EXECUTION DETAILS</b></h6>
    </div>
    <div class="row">
      <div class="col s4 m4">
        <b>RELEASE:</b>&emsp;<%= @testexecution.release_name %>
      </div>
      <div class="col s4 m4">
        <b>TESTCYCLE:</b>&emsp;<%= @testexecution.testcycle_name %>
      </div>
      <div class="col s4 m4">
        <b>Execution:</b>&emsp;<%= @status %>
      </div>
    </div>
    <div class="row">
      <div class="col s8 m8">
        <b>NAME:</b>&emsp;<%= @testexecution.testcase_name %>
      </div>
      <div class="col s4 m4">
        <b>TESTSUITE:</b>&emsp;<%= @testexecution.testsuite_name %>
      </div>
    </div>
    <div class="row">
      <div class="col s4 m4">
        <b>BROWSER:</b>&emsp;<%= @testexecution.browser %>
      </div>
      <div class="col s4 m4">
        <b>Operating System:</b>&emsp;<%= @testexecution.os %>
      </div>
    </div>
    <div class="row">
      <div class="col s8 m8">
        <b>USER:</b>&emsp;<%= @testexecution.testuser %>
      </div>
      <div class="col s4 m4">
        <b>PASSWORD:</b>&emsp;<%= @testexecution.testpassword %>
      </div>
    </div>
    <div class="row">
      <div class="col s8 m8">
        <b>TEST PATH:</b> <%= @testexecution.testpath %>
      </div>
      <div class="col s4 m4">
        <%= link_to 'Re-Run', {controller: 'testexecutions', action: 'submission', testcase: Testcase.find_by(title: @testexecution.testcase_name)} %>
      </div>
    </div>
  </div>
</div>
</div>
<div class="row">
  <div class="col s12 m12">
    <b>EXECUTION LOG:</b>&emsp;
    <textarea id="logtextarea" class="materialize-textarea"><%=@testexecution.runlog %></textarea>
  </div>
</div>
