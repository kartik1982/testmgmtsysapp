<div class="row">
  <div class="col s12 m12">
    <div class="card-panel">
      <div class="row header center">
      <h6><b>SCHEDULE DETAILS</b></h6>
    </div>
    <div class="row">
      <div class="col s6 m6">
        <b>NAME:</b>&emsp;<%= @schedule.job_name %>
      </div>
      <div class="col s6 m6">
        <b>TESTSUITE:</b>&emsp;<%= @schedule.testsuite_name %>
      </div>
    </div>
    <%= link_to '<i class="material-icons right">create</i> Edit'.html_safe, edit_schedule_path(@schedule), class: "btn waves-effect waves-light" %>
    <%= link_to '<i class="material-icons right">delete_forever</i> Delete'.html_safe, schedule_path(@schedule), method: :delete, data: {confirm: "Are you sure?"}, class: "btn waves-effect waves-light" if super_admin?%>
    <%= link_to '<i class="material-icons left">list</i> List Schedules'.html_safe, schedules_path, class: "btn waves-effect waves-light right" %>
    </div>
  </div>
</div>
